    "sass": "^1.49.9",
    "standard-version": "^9.3.2"
  },
  "devDependencies": {
    "husky": "^7.0.0",
    "rimraf": "^3.0.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
