{
  "name": "alpha-grid",
  "version": "1.3.0",
  "description": "Simple 12 column grid system build on css grid and inspired by Bootstrap ",
  "main": "dist/agrid.css",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "sass --watch src/agrid/scss/agrid.scss src/agrid/agrid.css",
    "build": "gulp build && gulp compress && gulp docs",
    "build:local": "node clean.js && gulp build && gulp compress",
    "prepare": "husky install",
    "release": "standard-version"
  },
  "keywords": [
    "12 column grid",
    "css grid",
    "grid layout"
  ],
  "author": {
    "name": "Simeon Petrov",
    "url": "https://github.com/simeon-petrov-5"
  },
  "homepage": "https://github.com/icanteven-code/alpha-grid",
  "license": "MIT",
  "dependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "autoprefixer": "^10.4.2",
    "gulp": "^4.0.2",
    "gulp-brotli": "^3.0.0",
    "gulp-csso": "^4.0.1",
    "gulp-gzip": "^1.4.2",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-sass": "^5.1.0",
    "gulp-size": "^4.0.1",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-useref": "^5.0.0",
    "postcss": "^8.4.6",
